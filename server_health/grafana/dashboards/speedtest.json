{
  "uid": "speedtest-main",
  "title": "SpeedTest Server",
  "timezone": "browser",
  "schemaVersion": 37,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "stat",
      "title": "Ping Requests Total",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "ping_requests_total",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justificationMode": "center"
      },
      "pluginVersion": "10.2.3",
      "gridPos": {"x": 0, "y": 0, "w": 4, "h": 4}
    },
    {
      "type": "stat",
      "title": "Downloaded Bytes Total",
      "datasource": "Prometheus",
      "targets": [
        {"expr": "download_bytes_total", "refId": "A"}
      ],
      "options": {"colorMode": "value", "graphMode": "none", "justificationMode": "center"},
      "pluginVersion": "10.2.3",
      "gridPos": {"x": 4, "y": 0, "w": 4, "h": 4}
    },
    {
      "type": "stat",
      "title": "Uploaded Bytes Total",
      "datasource": "Prometheus",
      "targets": [
        {"expr": "upload_bytes_total", "refId": "A"}
      ],
      "options": {"colorMode": "value", "graphMode": "none", "justificationMode": "center"},
      "pluginVersion": "10.2.3",
      "gridPos": {"x": 8, "y": 0, "w": 4, "h": 4}
    }
  ]
}
